{
  "name": "@swissgeol/ui-core-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:prepare && ng build && npm run build:share",
    "build:prepare": "tsx scripts/prepare-build.ts",
    "build:share": "tsx scripts/share-build.ts",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "sideEffects": false,
  "module": "fesm2022/swissgeol-core-angular.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "default": "./fesm2022/swissgeol-core-angular.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swisstopo/swissgeol-ui-core.git"
  },
  "files": [
    "package.json",
    "public-api.d.ts",
    "index.d.ts",
    "fesm2022",
    "lib"
  ],
  "dependencies": {
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.3",
    "@angular/compiler-cli": "^20.0.0",
    "@angular/cli": "^20.1.3",
    "@angular/common": "^20.1.3",
    "@angular/core": "^20.1.3",
    "@angular/forms": "^20.1.3",
    "ng-packagr": "^20.1.0"
  },
  "peerDependencies": {
    "@angular/common": "^20.1.3",
    "@angular/core": "^20.1.3",
    "@angular/forms": "^20.1.3",
    "@angular/router": "^20.1.3",
    "rxjs": "~7.8.2",
    "@swissgeol/ui-core": "*",
    "zone.js": "~0.15.1"
  }
}
