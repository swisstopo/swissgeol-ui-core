{
  "name": "swissgeol-core",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/swissgeol-core/swissgeol-core.esm.js",
  "exports": {
    ".": {
      "import": "./dist/swissgeol-core/swissgeol-core.esm.js",
      "require": "./dist/swissgeol-core/swissgeol-core.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "lint": "npx eslint && npx prettier --check .",
    "lint:fix": "npx eslint --fix && npx prettier . --write"
  },
  "devDependencies": {
    "@eslint/css": "^0.6.0",
    "@eslint/js": "^9.24.0",
    "@eslint/markdown": "^6.3.0",
    "@stencil/core": "^4.27.1",
    "@stencil/eslint-plugin": "^1.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-sort-class-members": "^1.21.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "prettier": "^3.5.3",
    "puppeteer": "^24.3.0",
    "stencil-inline-svg": "^1.1.0",
    "typescript-eslint": "^8.29.1"
  },
  "license": "MIT"
}
